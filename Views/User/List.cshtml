@model IEnumerable<GuestBook.Models.User>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>GuestBook</title>
</head>
<body>
        @foreach (var user in Model)
        {
            <div>
                <p>@user.GivenName</p>
                <p>@user.FamilyName</p>
            </div>
        }
<div>
    <a asp-controller="User" asp-action="AddUser">Add User</a>
</div>
</body>
</html>